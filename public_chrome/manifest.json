{
  "manifest_version": 3,
  "name": "Tab Dedup Manager",
  "version": "0.3.6",
  "description": "Tab Dedup Manager, groups and dedups tabs based on set of rules that user defines. Supports skipping and auto deleting.",
  "options_page": "index.html",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "View all available tests"
  },
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "incognito": "spanning",
  "permissions": ["storage", "tabs", "tabGroups"]
}
