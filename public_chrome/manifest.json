{
  "manifest_version": 3,
  "name": "Tab Dedup Manager",
  "version": "0.1.0",
  "description": "Tab Group Manager",
  "options_page": "index.html",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "View all available tests"
  },
  "background": {
    "service_worker": "./background.js",
    "type": "module"
  },
  "incognito": "spanning",
  "permissions": ["tabs", "tabGroups", "storage"]
}
