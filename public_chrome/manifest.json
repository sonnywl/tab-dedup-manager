{
  "manifest_version": 3,
  "name": "One-click Tab Dedup/Grouper Manager",
  "version": "0.4.12",
"description": "One click - groups and dedups tabs without worry",  "options_page": "index.html",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "View all available tests"
  },
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "incognito": "spanning",
  "permissions": ["storage", "tabs", "tabGroups"]
}
