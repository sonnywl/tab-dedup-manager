{
  "manifest_version": 3,
  "name": "Tab Dedup Manager",
  "version": "0.1.2",
  "description": "Customizable multi suggestion search tab viewer",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tab_dedup_manager@time_current.com",
      "update_url": "https://sea-lion-app-i977g.ondigitalocean.app/updates.json",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "action": {
    "default_icon": { "192": "logo192.png" },
    "default_title": "Tab Dedup Manager"
  },
  "background": {
    "scripts": ["./background.js"],
    "type": "module"
  },
  "incognito": "spanning",
  "permissions": ["tabs", "tabGroups"]
}
